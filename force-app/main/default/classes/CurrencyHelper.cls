    private map<Date, map<String,Decimal>> getExchangeRates(map<String,set<Date>> exchangeRateParams){
        String query = prepareExRateQuery(exchangeRateParams);
        list<Novidea_HPC__Exchange_Rate__c> exrates = (Novidea_HPC__Exchange_Rate__c[])database.query(query);
        return CurrencyHelper.populateExchangeRates(exrates);
    }
    
    private static map<Date, map<String,Decimal>> populateExchangeRates(list<Novidea_HPC__Exchange_Rate__c> exrates){
        map<Date, map<String,Decimal>> retmap = new map<Date, map<String,Decimal>>();
        map<String,Decimal> rates;
        for(Novidea_HPC__Exchange_Rate__c exrate: exrates){
            if((rates=retmap.get(exrate.Novidea_HPC__Rate_Date__c))==null){
                rates = new map<String,Decimal>();
            }
            rates.put(exrate.Novidea_HPC__Target_Currency__c, exrate.Novidea_HPC__Rate__c);
            retmap.put(exrate.Novidea_HPC__Rate_Date__c,rates);
        }
        return retmap;
    }
    
    private String prepareCurrencies(String csv){
        String retval='';
        String[] arr = csv.split(',');
        for(String val:arr){
            retval+='\'' + val+'\',';
        }
        if(!''.equals(retval))retval=retval.substring(0,retval.length()-1);
        return retval;
    }
    private String listToCsv(set<Date> dateSet){
        String retval = '';
        for(Date d:dateSet){
            retval+=Datetime.valueOf(d+'').format('yyyy-MM-dd') + ',';
        }
        if(!''.equals(retval))retval=retval.substring(0,retval.length()-1);
        return retval;
    }
    //limit of 10000 chars for dynamic query - https://na14.salesforce.com/help/doc/en/salesforce_app_limits_cheatsheet.pdf
    private String prepareExRateQuery(map<String,set<Date>> exchangeRateParams){
        String tempQueryPart='', whereQueryPart='', currencies;
        String query = 'SELECT Novidea_HPC__Rate__c, Novidea_HPC__Rate_Date__c, Novidea_HPC__Source_Currency__c, Novidea_HPC__Target_Currency__c\n'+
                        'FROM Novidea_HPC__Exchange_Rate__c\n'+
                        'WHERE Novidea_HPC__Source_Currency__c=\'' + BASE_CURRENCY + '\'\nAND(\n';
        for(String key: exchangeRateParams.keyset()){
            currencies = prepareCurrencies(key);
            tempQueryPart = listToCsv(exchangeRateParams.get(key));
            whereQueryPart += '(Novidea_HPC__Rate_Date__c IN(' + tempQueryPart + ') AND Novidea_HPC__Target_Currency__c IN (' + currencies + '))\n';
            whereQueryPart+='OR';
            //TODO:Handle long queries
            if(whereQueryPart.length()>9500){system.debug('EXIT');break;}
        }
        whereQueryPart = whereQueryPart.substring(0,whereQueryPart.length()-'OR'.length()) + ')' ;
        query+=whereQueryPart;
        system.debug('\n' + query);
        return query;
    }
    
