    public sObject[] updateCurrencyFields(sObject[] objs){
        if(objs==null || objs.isempty() || CurrencyHelper.currencyFieldNamesByTargetField==null || CurrencyHelper.currencyFieldNamesByTargetField.isEmpty())return objs;
        //get a list of CurrencyFields for source fields
        //get a list of exchange rate for the required dates and currencies
        ConvertData convertData = getConversionData(objs, CurrencyHelper.currencyFieldNamesByTargetField);
        //currencyFieldNamesByTargetField = null;
        system.debug(logginglevel.error,convertData);
        //get the relevant exchangeRates: source currency,target currency==>set of rereuired dates
        map<String,set<Date>> exchangeRateParams = convertData.exchangeRateParams;
        //date=>source==>target
        map<Date, map<String,Decimal>> rates = getExchangeRates(exchangeRateParams);
        if(rates==null || rates.isEmpty()){
        	system.debug(LoggingLevel.ERROR, 'No Exchange Rates Found');
            String msg;
            if(objs.size()==1){
                msg = 'לא נמצאו שערי חליפין עבור {0}';
            }
            else{
                msg = 'לא נמצאו שערי חליפין עבור אף אחד ממופעי {0}.';
            }
            errors.add(String.format(msg,new String[]{objs[0].getSObjectType().getDescribe().getLocalName()}));
            
            if(!system.isBatch()){
                Sobject obj = (objs.size()==1?objs[0]:null);
                handleErrors(obj);
            }
            else if(errors.size()>1000) handleErrors(null);
            return objs;
       }
        //compute the target fields values and update on the given objects
        map<Integer,Sobject> objMap = convertData.objMap;
        
        for(Integer key:convertData.conversionFields.keyset()){
            system.debug(key);
            for(String targetField: convertData.conversionFields.get(key).keyset()){
                system.debug(targetField);
                objMap.get(key).put(targetField, convert(targetField, convertData.conversionFields.get(key).get(targetField),rates, objMap.get(key))); 
            }
        }
        system.debug(errors);
        if(!system.isBatch()){
            Sobject obj = objs.size()==1?objs[0]:null;
            handleErrors(obj);
        }
        else if(errors.size()>1000) handleErrors(null);
        return objMap.values();
    }
