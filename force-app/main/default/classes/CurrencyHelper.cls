    private boolean handleErrors(sObject obj){
        if(errors!=null && !errors.isEmpty()){
            String errMsgs = '';
            for(String err: errors) errMsgs+= (err + '\n');
            errors = new set<String>();
            if(system.isBatch())throw new UpdateCurrencyValuesException(errMsgs);
            else sendCurrencyUpdateFailureEmail(obj, errMsgs);
            return false;
        }
        return true;
    }
    
    protected virtual void sendCurrencyUpdateFailureEmail(sObject obj, String msg){
        if(null==CURRENCY_MANAGER_ADMIN_EMAIL)return;
        Messaging.reserveSingleEmailCapacity(1);
        Messaging.SingleEmailMessage mail = new Messaging.SingleEmailMessage();
        mail.setCharset('UTF-8');
        mail.setToAddresses(new String[]{CURRENCY_MANAGER_ADMIN_EMAIL});
        EmailTemplate emailtemplate = [SELECT Id, Subject, Body FROM EmailTemplate WHERE DeveloperName='Currency_Fields_Calculation_Failure'];
        mail.setSubject(String.format(emailtemplate.Subject,new String[]{objName}));
        String[] formatArray = new String[]{objName};
        String singleObjDetails = '';
        if(obj!=null && obj.Id!=null) {
        	singleObjDetails += (' מזהה: ' + obj.Id + '\n');
        	singleObjDetails += (URL.getSalesforceBaseUrl().toExternalForm() + '/' +  obj.Id + '\n');
        }
        if(obj!=null && obj.get('Name')!=null) singleObjDetails += (' שם: ' + (String)obj.get('Name'));
        formatArray.add(singleObjDetails);
        formatArray.add(msg);
        mail.setPlainTextBody(String.format(emailtemplate.Body,formatArray));
        Messaging.sendEmail(new Messaging.SingleEmailMessage[] { mail });
    }
    
