    public static Decimal convertValue(Date valueDate, Decimal value, String srcCurrency, String targetCurrency){
        if(srcCurrency.equalsIgnoreCase(targetCurrency))return value;
        Decimal srcRate, targetRate;

        if(!BASE_CURRENCY.equalsIgnoreCase(srcCurrency)){
            list<Novidea_HPC__Exchange_Rate__c>  exratesSource = [SELECT Novidea_HPC__Rate__c
                                        FROM Novidea_HPC__Exchange_Rate__c 
                                        WHERE Novidea_HPC__Rate_Date__c <=:valueDate
                                        AND Novidea_HPC__Target_Currency__c =:srcCurrency
                                        ORDER BY Novidea_HPC__Rate_Date__c DESC
                                        LIMIT 1];
            if(!exratesSource.isEmpty())srcRate = exratesSource[0].Novidea_HPC__Rate__c;
        }
        else srcRate = 1;
        if(!BASE_CURRENCY.equalsIgnoreCase(targetCurrency)){
            list<Novidea_HPC__Exchange_Rate__c>  exratesTarget = [SELECT Novidea_HPC__Rate__c
                                        FROM Novidea_HPC__Exchange_Rate__c 
                                        WHERE Novidea_HPC__Rate_Date__c <=:valueDate
                                        AND Novidea_HPC__Target_Currency__c =:targetCurrency
                                        ORDER BY Novidea_HPC__Rate_Date__c DESC
                                        LIMIT 1];
                system.debug('valueDate: ' + valueDate + ' targetCurrency: ' + targetCurrency + ' srcCurrency: ' + srcCurrency);

            if(!exratesTarget.isEmpty())targetRate = exratesTarget[0].Novidea_HPC__Rate__c;
        }
        else targetRate = 1;
        system.debug(srcRate + ' ' + targetRate);
        if(srcRate==null || targetRate==null) return null;

        //get the value in base currency
        Decimal baseCurrencyValue = value * srcRate;
        if(BASE_CURRENCY.equalsIgnoreCase(targetCurrency))return baseCurrencyValue;

        Decimal retval = baseCurrencyValue/targetRate;
        retval = retval.setScale(2);
        return retval;  
    }
 
    /*covert to base currency*/
    public static Decimal convertValue(Date valueDate, Decimal value, String srcCurrency){
        if(BASE_CURRENCY.equalsIgnoreCase(srcCurrency))return value;
        list<Novidea_HPC__Exchange_Rate__c> exrates = [SELECT Novidea_HPC__Rate__c 
                                        FROM Novidea_HPC__Exchange_Rate__c 
                                        WHERE Novidea_HPC__Rate_Date__c <=:valueDate
                                        AND Novidea_HPC__Target_Currency__c =:srcCurrency
                                        ORDER BY Novidea_HPC__Rate_Date__c DESC
                                        LIMIT 1];
                                        
        if(!exrates.isEmpty())return value*exrates[0].Novidea_HPC__Rate__c;
        return null;    
    }
