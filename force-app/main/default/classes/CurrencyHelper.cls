    public ConvertData getConversionData(sObject[] objs, map<String,CurrencyField> currencyFieldNamesByTargetFieldMap){
        map<Integer,map<String,CurrencyFieldValues>> retmap = new map<Integer,map<String,CurrencyFieldValues>>();
        //source,target=>set<valueDates>
        map<String,set<Date>> exchangeRate = new map<String,set<Date>>();
        
        map<Integer,Sobject> objMap = new map<Integer,Sobject>();
        map<String,CurrencyFieldValues> sourceDataByTarget;
        Date tempDate;
        set<Date> tempValueDateSet;
        String tempSourceCurrency, tempTargetCurrency, keyval, tempSourceCurrencyFieldName, tempSourceAmountFieldName;
        boolean firstCompare;
		//rz change 30/01/2013: added in order to id new sObjects as opposed to updated
        HandledObjectList handledObjs = new HandledObjectList(objs);
        //iterate through the fields to be converted
        for(String key: currencyFieldNamesByTargetFieldMap.keyset()){
            tempSourceCurrencyFieldName = currencyFieldNamesByTargetFieldMap.get(key).sourceCurrencyFieldName;
            tempSourceAmountFieldName = currencyFieldNamesByTargetFieldMap.get(key).sourceAmountFieldName;
            tempTargetCurrency = getCoinPart(key);
            for(HandledObject handledObj: handledObjs.handledObjects){
            	sObject obj = handledObj.obj;
                tempDate = getValueDate(obj, valueDateFieldName);
                tempSourceCurrency = (String)obj.get(tempSourceCurrencyFieldName);
                if((sourceDataByTarget=retmap.get(handledObj.Id))==null)
                    sourceDataByTarget = new map<String,CurrencyFieldValues>();
                sourceDataByTarget.put(key, new CurrencyFieldValues((Decimal)obj.get(tempSourceAmountFieldName), 
                                                                    tempSourceCurrency, 
                                                                    tempDate
                                                                    )
                                        );
                retmap.put(handledObj.Id,sourceDataByTarget);
                if((firstCompare =(tempValueDateSet=exchangeRate.get(tempSourceCurrency + ',' + tempTargetCurrency))==null) && 
                                    (tempValueDateSet=exchangeRate.get(tempTargetCurrency + ',' + tempSourceCurrency))==null)
                    tempValueDateSet = new set<Date>();
                tempValueDateSet.add(tempDate);
                if(firstCompare) keyval = tempTargetCurrency + ',' + tempSourceCurrency;
                else keyval = tempSourceCurrency + ',' + tempTargetCurrency;
                exchangeRate.put(keyval, tempValueDateSet);
                objMap.put(handledObj.Id,obj);
            }
        }
        return new ConvertData(retmap, exchangeRate, objMap);
    }
    
