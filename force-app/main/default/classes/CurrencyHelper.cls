    //maps the currency FiledsName & matching field (the original field) by the target field (the one that shoud be converted)
    private map<String,CurrencyField> getCurrencyFieldNames(set<String> objfields){
        map<String,CurrencyField> currencyFieldNamesByTargetFieldMap = new map<String,CurrencyField>();
        for(String key: objfields){
            if(!isFieldCoin(key))continue;
            String matchingField = getMatchingFieldName(key, objfields);
            System.debug('key:' + key + ', matchingField:' + matchingField);
            currencyFieldNamesByTargetFieldMap.put(key,new CurrencyField(getCurrencyFieldName(matchingField, currencyCustomSettings) , matchingField));
        }
        return currencyFieldNamesByTargetFieldMap;
    }

    private String getCoinPart(String fieldName){
        if(fieldName==null || !CURRENCY_FIELD_PATTERN.matcher(fieldName).matches()) return null;
        return fieldName.substring(fieldName.length()-'XXX__c'.length(),fieldName.length()-'__c'.length()).toUpperCase();
    }

    private boolean isFieldCoin(String fieldName){
        String coinPart = getCoinPart(fieldName);
        if(coinPart==null || ''.equals(coinPart))return false;
        return CURRENCY_ABRV_SET.contains(coinPart.toLowerCase()) || CURRENCY_ABRV_SET.contains(coinPart.toUpperCase());
    }

    /*
        gets the field name that controls the currency of the given field. i.e. the matchingField arg.
    */
    private String getCurrencyFieldName(String matchingField, Sobject currencyCustomSettings){
        String retval;
        try {
        	retval = (String)currencyCustomSettings.get(matchingField);
        } catch (Exception e){
        	// Patch: was done because of a terrible mistake involving package, custom settings, and custom settings visibility property
        	retval = (String)currencyCustomSettings.get(matchingField = matchingField.replace('Novidea_HPC__','').replace('novidea_hpc__',''));
        }
        system.debug('currencyCustomSettings:' + currencyCustomSettings);
        system.debug('matchingField: ' + matchingField + ' retval: ' + retval + ' currencyCustomSettings: ' + currencyCustomSettings);
        return concatPackage(retval);
    }
    
    /*
        gets the a field name which equals the target name without the coin part
    */
    private String getMatchingFieldName(String targetFieldName, set<String> objfields){
        String coinPart = getCoinPart(targetFieldName);
        String lowerName = targetFieldName.toLowerCase();
        String retval = lowerName.substring(0,lowerName.lastIndexOf('_' + coinPart.toLowerCase() + '__c'))+'__c';
        if (objfields.contains(retval))
        	return retval;
        return concatPackage(retval);
    }
    
    private String concatPackage(String fieldName){
        if(!fieldName.startsWith('Novidea_HPC__'))fieldName='Novidea_HPC__' +fieldName;
        return fieldName;
    }
    
    private String unconcatPackage(String fieldName){
        if(fieldName.startsWith('Novidea_HPC__'))fieldName=fieldName.substring('Novidea_HPC__'.length());
        return fieldName;
    }
    
    private Decimal convert(String targetFieldName, CurrencyFieldValues sourceField, map<Date, map<String,Decimal>> rates, sObject obj){
        String targetCurrency = getCoinPart(targetFieldName);
        if(targetCurrency.equalsIgnoreCase(sourceField.crncy))return sourceField.value;
        if(!rates.containsKey(sourceField.valueDate)){
            addConvertError(sourceField.crncy + ' and ' + targetCurrency, sourceField.valueDate, targetFieldName, (String)obj.get('Name'), Obj.Id);
            System.debug(sourceField.crncy + ' and ' + targetCurrency + ' ' + sourceField.valueDate + ' ' + targetFieldName + ' ' + (String)obj.get('Name') + ' ' + Obj.Id);
            return null;
        }
        Decimal baseCurrencyValue = 0;
        if (sourceField.value != null) {
	        //when the source currency isn't the base currency 
	        if(!BASE_CURRENCY.equalsIgnoreCase(sourceField.crncy)){
	            if(!rates.get(sourceField.valueDate).containsKey(sourceField.crncy)){
	                addConvertError(sourceField.crncy, sourceField.valueDate, targetFieldName, (String)obj.get('Name'), Obj.Id);
	                System.debug(sourceField.crncy + ' and ' + targetCurrency + ' ' + sourceField.valueDate + ' ' + targetFieldName + ' ' + (String)obj.get('Name') + ' ' + Obj.Id);
	                return null;
	            }
	            //get the value in base currency
	            baseCurrencyValue = sourceField.value * rates.get(sourceField.valueDate).get(sourceField.crncy);
	        }
	        else{
	            baseCurrencyValue = sourceField.value;
	        }
        }
        
        if(BASE_CURRENCY.equalsIgnoreCase(targetCurrency))return baseCurrencyValue;
        if(!rates.get(sourceField.valueDate).containsKey(targetCurrency)){
            addConvertError(targetCurrency, sourceField.valueDate, targetFieldName, (String)obj.get('Name'), Obj.Id);
            System.debug(sourceField.crncy + ' and ' + targetCurrency + ' ' + sourceField.valueDate + ' ' + targetFieldName + ' ' + (String)obj.get('Name') + ' ' + Obj.Id);
            return null;
        }
        system.debug('baseCurrencyValue: ' + baseCurrencyValue + ' rates: ' + rates + ' sourceField: '+ sourceField + ' targetCurrency:'+targetCurrency);
        return baseCurrencyValue/rates.get(sourceField.valueDate).get(targetCurrency);
    }

    private void addConvertError(String crncy, Date exRateDate, String targetFieldName, String objName, String ObjId){
        String errTemplate = 'לא נמצאו שערים עבור מטבע: {0} לתאריך: {1}.\n';
        errTemplate+= 'לכן לא ניתן לחשב את ערך השדה {2} עבור {3} עם מזהה: {4}.';
        errors.add(String.format(errTemplate,new String[]{crncy,exRateDate.format(),targetFieldName,objName,ObjId}));
    }
    /*
        gets the value date from the object
    */
    private Date getValueDate(sObject obj, String valueDateField){
        Date retDate;
        if(valueDateField==null || ''.equals(valueDateField) || obj.get(valueDateField)==null) return getValueDate(obj);
        if(obj.get(valueDateField) instanceof Datetime){
            Datetime val = (Datetime)obj.get(valueDateField);
            return val.date();
        }
        return (Date)obj.get(valueDateField);
    }
    
        
    private Date getValueDate(sObject obj){
        Datetime createddate;
        if(obj.get('CreatedDate')==null)
            //cover the before insert scenario were created date is null yet
            createddate = Datetime.now();
        else
            createddate = (Datetime)obj.get('CreatedDate');
        return createddate.date();
    }   
    
    //global Date
    //TODO: problem with ORDER by IN query which may result in too many dates to update in subsequent exchange rate extraction query creation
    global Database.QueryLocator start(Database.BatchableContext info)
    { 
    	Schema.Describesobjectresult objDesc = Schema.getGlobalDescribe().get(objName).getDescribe();
    	map<String,Schema.Sobjectfield> objfields = objDesc.fields.getMap();
        init(objDesc);
        String selectFields = 'Name, CreatedDate,', queryOrderBy='ORDER BY ';
        if(valueDateFieldName!=null){
            String fn = valueDateFieldName;
            selectFields+= fn + ',\n';
            queryOrderBy +=fn;
        }
        else queryOrderBy += 'CreatedDate';
        String query = 'SELECT ';
        set<String> fieldNames = new set<String>();
        String whereText = '';
        Set<String> whereTextParts = new Set<String>();
        for(String target: CurrencyHelper.currencyFieldNamesByTargetField.keyset()){
            CurrencyField cf = CurrencyHelper.currencyFieldNamesByTargetField.get(target);
            String sourceCurrencyFieldName;
            if (objfields.keyset().contains(cf.sourceCurrencyFieldName))
            	sourceCurrencyFieldName = cf.sourceCurrencyFieldName;
            else
            	sourceCurrencyFieldName = concatPackage(cf.sourceCurrencyFieldName);
            if (sourceCurrencyFieldName != null)
            	fieldNames.add(sourceCurrencyFieldName);
            
            String sourceAmountFieldName; 
            if (objfields.keyset().contains(cf.sourceAmountFieldName))
            	sourceAmountFieldName = cf.sourceAmountFieldName;
            else 
            	sourceAmountFieldName = concatPackage(cf.sourceAmountFieldName);
            if (sourceAmountFieldName != null)
            	fieldNames.add(sourceAmountFieldName);
            if (sourceAmountFieldName != null && sourceCurrencyFieldName != null && target != null)
            	whereTextParts.add(sourceAmountFieldName + ' != null And ' + sourceCurrencyFieldName + ' != null ' + ' And ' + target + ' = null' );
            selectFields+= target + ',\n';
        }
        
        for (String text:whereTextParts) {
        	if (whereText != '')
        		whereText += ' Or ';
        	whereText += '(' + text + ')'; 
        }
        whereText = '(' + whereText + ')';
        
        for(String val: fieldNames){
            selectFields+=val+',\n';
        }
        selectFields = selectFields.substring(0, selectFields.length()-2);
        query+=selectFields + '\n' +'FROM ' + objName;
         
        if (whereText != '' && runOnlyOnRecordsWithNoData)
        	query+= '\n' + ' Where ' + whereText;
        query+= '\n' + queryOrderBy;
        system.debug(query);
        return Database.getQueryLocator(query);
    }
    
    private void init(Schema.Describesobjectresult objDesc){
        if (CurrencyHelper.currencyFieldNamesByTargetField == null) {
            map<String,Schema.Sobjectfield> objfields = objDesc.fields.getMap();
            //get a map of target to currency type
            CurrencyHelper.currencyFieldNamesByTargetField = getCurrencyFieldNames(objfields.keyset());
        }
    }
    
