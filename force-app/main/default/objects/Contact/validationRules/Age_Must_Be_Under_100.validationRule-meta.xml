<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_Must_Be_Under_100</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT($Setup.Novidea_HPC__Trigger__c.Novidea_HPC__Stop_Validation_Rule__c ),
( AgentWise_IL__Age__c)  &gt;= 100)</errorConditionFormula>
