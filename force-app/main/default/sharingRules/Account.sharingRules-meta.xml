    <sharingCriteriaRules>
        <fullName>Allow_for_Mega_Users_except_RI</fullName>
        <accessLevel>Edit</accessLevel>
        <accountSettings>
            <caseAccessLevel>None</caseAccessLevel>
            <contactAccessLevel>Edit</contactAccessLevel>
            <opportunityAccessLevel>None</opportunityAccessLevel>
        </accountSettings>
        <description>כל הרשומות פתוחות למעט סוג רשומה RI</description>
        <label>Allow for Mega Users except RI</label>
        <sharedTo>
            <group>AllowedForAllRecordsExceptRI</group>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>notEqual</operation>
            <value>RI</value>
        </criteriaItems>
        <criteriaItems>
            <field>Nilibit_Status__c</field>
            <operation>notEqual</operation>
            <value>נוצר מניליביט</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
